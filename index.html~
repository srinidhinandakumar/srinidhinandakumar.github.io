	<script>
		$( document ).ready(function(){
			$(".button-collapse").sideNav()
			$('.scrollspy').scrollSpy();
			$('.collapsible').collapsible();
			$(window).scroll(function(){
			
				//console.log($(window).scrollTop());
				if($(window).scrollTop()>0){
					$("#check").addClass("pinned");
					$("#check").removeClass("pin-top");
					if($(window).width()<400)
					{
						$("#nav_div").addClass("navbar-fixed");
					}
					else
						$("#nav_div").removeClass("navbar-fixed");
					
				}
				else
					$("#check").removeClass("pinned");
					$("#check").addClass("pin-top");
					if($(window).width()<400)
					{
						$("#nav_div").addClass("navbar-fixed");
					}
					else
					$("#nav_div").removeClass("navbar-fixed");
					
			});
			
			var width=$(window).width();
			//console.log(width);
			if($(window).width()<400)
			{
				//console.log($(window).width());
				
				$("#title_nav").addClass("fontChange");
				$("#contact_icon1").removeClass("fa-2x");
				$("#contact_icon1").addClass("fa-lg");
				$("#contact_icon2").removeClass("fa-2x");
				$("#contact_icon2").addClass("fa-lg");
				$("#contact_icon3").removeClass("fa-2x");
				$("#contact_icon3").addClass("fa-lg");
				$("#contact_icon4").removeClass("fa-2x");
				$("#contact_icon4").addClass("fa-lg");
				
				
			}
			else
			{
				
				$("#title_nav").removeClass("fontchange");
				$("#contact_icon1").removeClass("fa-lg");
				$("#contact_icon1").addClass("fa-2x");
				$("#contact_icon2").removeClass("fa-lg");
				$("#contact_icon2").addClass("fa-2x");
				$("#contact_icon3").removeClass("fa-lg");
				$("#contact_icon3").addClass("fa-2x");
				$("#contact_icon4").removeClass("fa-lg");
				$("#contact_icon4").addClass("fa-2x");
			}
		});
		
	</script>
